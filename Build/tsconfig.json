{
	"compilerOptions": {
		"target": "es2020",
		"allowSyntheticDefaultImports": true,
		"incremental": true,
		"tsBuildInfoFile": "./.cache/tsconfig.tsbuildinfo",
		"module": "es2020",
		"moduleResolution": "node",
		"sourceMap": false,
		"removeComments": false,
		"alwaysStrict": true,
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"useDefineForClassFields": false,
		"noImplicitAny": false,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"pretty": true,
		"baseUrl": "Sources/TypeScript",
		"paths": {
			"@typo3-core/*": [
				"../../../.Build/vendor/typo3/cms-core/Resources/Public/JavaScript/*"
			],
			"@typo3-backend/*": [
				"../../../.Build/vendor/typo3/cms-backend/Resources/Public/JavaScript/*"
			],
		},
		"outDir": "./JavaScript/",
		"typeRoots": [
			"node_modules/@types",
			"types"
		]
	},
	"exclude": [
		"composer/"
	]
}
